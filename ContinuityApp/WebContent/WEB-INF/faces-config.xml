<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
  <render-kit>
    <renderer>
      <component-family>javax.faces.SelectOne</component-family>
      <renderer-type>com.teamstudio.renderers.SelectOneType</renderer-type>
      <renderer-class>com.teamstudio.renderers.SelectOne</renderer-class>
    </renderer>
  </render-kit>
  <managed-bean id="dBar">
    <managed-bean-name>dBar</managed-bean-name>
    <managed-bean-class>eu.linqed.debugtoolbar.DebugToolbar
		</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <!-- persistent logs to an external (OpenLog)/ current database -->
    <managed-property>
      <property-name>logDbPath</property-name>
      <value>current</value>
      <!-- can also be the full path to the log database, e.g. logs\openLog.nsf -->
    </managed-property>
    <managed-property>
      <property-name>logEnabled</property-name>
      <value>false</value>
    </managed-property>
    <managed-property>
      <property-name>logLevel</property-name>
      <value>info</value>
    </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>configBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.Configuration</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>siteBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.Site</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>roleBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.Role</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>responsibilityBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.Responsibility</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>contactBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.apps.Contact</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>contactsImportBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.apps.ContactsImport</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>scenarioBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.Scenario</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>taskBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.Task</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>currentUserBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.User</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>fileBean</managed-bean-name>
    <managed-bean-class>com.teamstudio.continuity.File</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <!-- phase listeners -->
  <lifecycle>
    <phase-listener>com.teamstudio.continuity.utils.MultiPageMessages</phase-listener>
  </lifecycle>
  <!--AUTOGEN-START-BUILDER: Automatically generated by IBM Domino Designer. Do not modify.-->
  <!--AUTOGEN-END-BUILDER: End of automatically generated section-->
</faces-config>
